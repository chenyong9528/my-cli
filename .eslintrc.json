{
  "env": {
    "browser": true,
    "node": true,
    "es2020": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error", { "endOfLine":"auto" }],
    "no-console": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
